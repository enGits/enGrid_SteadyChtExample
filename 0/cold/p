FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

value           0;
internalField   uniform $value;

boundaryField
{
    ".*"
    {
        type            calculated;
        value           uniform $value;
    }
}


