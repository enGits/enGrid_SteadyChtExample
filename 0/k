FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

value           1.5e-2;
internalField   uniform $value;

boundaryField
{
    ".*"
    {
        type            calculated;
        value           uniform $value;
    }
}


